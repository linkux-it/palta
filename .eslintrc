{
    "root": true,
    "parser": "babel-eslint",
    "settings": {
        "ecmascript": 7
    },
    "ecmaFeatures": {
        "blockBindings": true,
        "forOf": true,
        "jsx": true,
        "generators": true,
        "destructuring": true,
        "classes": true,
        "defaultParams": true,
        "modules": true,
        "binaryLiterals": true
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "prefer-reflect": 2
    },
    "env": {
        "node": true
    }
}
